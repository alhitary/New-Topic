{
	"name": "dmzx/newtopic",
	"type": "phpbb-extension",
	"description": "Show the list of forums on the board, the forum selected will be created a new topic.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.1",
	"time": "2015-11-19",
	"keywords": ["phpbb", "extension", "newtopic"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "New Topic",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.2.*@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "newtopic_version.json"
		}
	}
}